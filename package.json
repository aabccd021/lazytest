{
  "scripts": {
    "build": "rm -rf cjs esm && echo '{ \"extends\": \"./tsconfig.json\", \"include\": [\"./src\"] }' > tsconfig.dist.json && tsc --project tsconfig.dist.json --declaration --declarationMap --skipLibCheck --module commonjs --outDir cjs && tsc --project tsconfig.dist.json --declaration --declarationMap --skipLibCheck --moduleResolution node --module esnext --outDir esm && rm tsconfig.dist.json",
    "test": "exec-main ./test/index",
    "lint": "eslint . --max-warnings 0 --ignore-path .gitignore",
    "lint:full": "pnpm lint --rule 'import/no-cycle: error' --rule 'import/namespace: error' --rule '@typescript-eslint/no-misused-promises: error'",
    "lint:fix": "pnpm lint --fix"
  },
  "sideEffects": false,
  "name": "pure-test",
  "repository": "https://github.com/aabccd021/pure-test",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "files": [
    "cjs",
    "esm"
  ],
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./cjs/index.d.ts",
  "exports": {
    "./cjs/node18": {
      "require": "./cjs/node18.js",
      "import": "./esm/node18.js"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "eslintConfig": {
    "extends": [
      "chimeraant"
    ],
    "rules": {
      "import/no-cycle": "off",
      "import/namespace": "off"
    },
    "overrides": [
      {
        "files": "src/node18.ts",
        "rules": {
          "import/no-nodejs-modules": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@morphic-ts/batteries": "^3.0.0",
    "@morphic-ts/model-algebras": "^3.0.0",
    "@morphic-ts/summoners": "^3.0.0",
    "diff": "^5.1.0",
    "fp-ts": "^2.13.1",
    "fp-ts-std": "^0.15.1",
    "picocolors": "^1.0.0",
    "retry-ts": "^0.1.4",
    "serialize-error": "^11.0.0",
    "spectacles-ts": "^1.0.7",
    "ts-pattern": "^4.0.6",
    "tsimportlib": "^0.0.3"
  },
  "devDependencies": {
    "@types/diff": "^5.0.2",
    "@types/node": "^18.11.18",
    "c8": "^7.12.0",
    "eslint": "^8.30.0",
    "eslint-config-chimeraant": "^1.2.9",
    "exec-main": "^1.0.1",
    "pnpm": "^7.25.0",
    "semantic-release": "^20.0.2",
    "ts-essentials": "^9.3.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
